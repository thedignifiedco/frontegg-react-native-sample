{
  "expo": {
    "name": "frontegg-react-native-sample",
    "slug": "frontegg-react-native-sample",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fronteggreactnativedemo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.frontegg-react-native-sample"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.fronteggreactnativesample"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "fronteggBaseUrl": "https://dignifiedlabs-dev.frontegg.com",
      "fronteggClientId": "9832d283-6401-4a61-bdd7-b6d842032688",
      "fronteggRedirectUri": "fronteggreactnativedemo://callback",
      "fronteggLogoutRedirectUri": "fronteggreactnativedemo://logout"
    }
  },
  "name": "FronteggReactNativeDemo",
  "slug": "FronteggReactNativeDemo"
}
